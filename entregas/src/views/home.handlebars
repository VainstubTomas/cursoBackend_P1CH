<div>
    <h1>Lista de productos</h1>

    {{#each products}}
        <div class="productCard">
            <li>
                <img src={{this.thumbnails}} alt="imagen producto" class="productImage">
                <h2>{{this.title}}</h2>
                <h3>Precio: ${{this.price}}</h3>
            </li>
        </div>
    {{/each}}
</div>

<script src="/socket.io/socket.io.js"></script>

<script>
    const socket = io();
</script>